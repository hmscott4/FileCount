      <ProbeActionModuleType ID="FileCount.FileCount.DataSource.PSScript" Accessibility="Public" Batching="false" PassThrough="false" RunAs="FileCount.RunasProfile">
        <Configuration>
          <xsd:element minOccurs="0" name="ID" type="xsd:integer" xmlns:xsd="http://www.w3.org/2001/XMLSchema" />
          <xsd:element minOccurs="0" name="CSV" type="xsd:string" xmlns:xsd="http://www.w3.org/2001/XMLSchema" />
        </Configuration>
        <ModuleImplementation Isolation="Any">
          <Composite>
            <MemberModules>
              <ProbeAction ID="PSScript" TypeID="Windows!Microsoft.Windows.PowerShellPropertyBagTriggerOnlyProbe">
                <ScriptName>GetFileCount.ps1</ScriptName>
                <ScriptBody>
                  param($ID,$CSV)

                  $ScriptName = "GetFileCount.ps1"
                  $EventID = 3521

                  # Starting Script section - All scripts get this
                  #=================================================================================
                  # Gather the start time of the script
                  $StartTime = Get-Date
                  #Set variable to be used in logging events
                  $whoami = whoami
                  # Load MOMScript API
                  $momapi = New-Object -comObject MOM.ScriptAPI
                  # Load PropertyBag function
                  $bag = $momapi.CreatePropertyBag()
                  #Log script event that we are starting task
                  $momapi.LogScriptEvent($ScriptName,$EventID,0,"`n Script is starting. `n Running as ($whoami).")
                  #=================================================================================

                  $shares = Import-csv -path $csv
                  #Filter Shares based on ID
                  # $shares
                  $share = $shares | where ID -EQ $ID


                  #Declare variables containing extension to check and file age

                  if($share.extension)
                  {
                  [string]$strExtension = "*"+$share.extension
                  }
                  else
                  {
                  [string]$strExtension = "*"
                  }

                  [string]$strAge = $share.time
                  [string]$strShare = $share.Share
                  [int]$intCount = $share.count
                  [int]$intRecurse = $share.recurse

                  # Test to see if Share is Accessible
                  If(Test-Path -path "$strShare" -pathType Container -ErrorAction SilentlyContinue)
                  {
                  # Count all files with the specific extension and older then the given time
                  if ($intRecurse -eq 1)
                  {
                  $count  = Get-ChildItem -Recurse $strShare\$strExtension | where{$_.LastWriteTime -le (Get-Date).AddMinutes($strAge)}|Measure-Object |%{$_.Count}

                  if ($count -le $intCount)
                  {
                  $strStatus = 1
                  $strMessage="Number of Files ($count) in $strShare is under threshold $intCount."
                  $momapi.LogScriptEvent($ScriptName,$EventID,2,"`n Found $count file(s) with extension $strExtension in $strShare. `n Under threshold $intCount.")
                  }
                  else
                  {
                  $strMessage="Number of Files ($count) in $strShare exceeds threshold $intCount."
                  $momapi.LogScriptEvent($ScriptName,$EventID,2,"`n Found $count file(s) with extension $strExtension in $strShare.`n Exceeds threshold $intCount.")
                  $strStatus = 0
                  }
                  }
                  else
                  {
                  $count  = Get-ChildItem $strShare\$strExtension | where{$_.LastWriteTime -le (Get-Date).AddMinutes($strAge)}|Measure-Object |%{$_.Count}

                  if ($count -le $intCount)
                  {
                  $strMessage="Number of Files ($count) in $strShare is under threshold $intCount."
                  $momapi.LogScriptEvent($ScriptName,$EventID,0,"`n Found $count file(s) with extension $strExtension in $strShare. `n Under threshold $intCount.")
                  $strStatus = 1
                  }
                  else
                  {

                  $strMessage="Number of Files ($count) in $strShare exceeds threshold $intCount."
                  $momapi.LogScriptEvent($ScriptName,$EventID,2,"`n Found $count file(s) with extension $strExtension in $strShare.`n Exceeds threshold $intCount.")
                  $strStatus = 0
                  }
                  }
                  }

                  Else
                  {
                  # Unable to access File Share
                  $strMessage="Unable to access $strShare"
                  $momapi.LogScriptEvent($ScriptName,$EventID,2,"`n Unable to access $strShare as $whoami.")
                  $count = 0
                  $strStatus = 0
                  }

                  # End of script section
                  #=================================================================================
                  #Log an event for script ending and total execution time.
                  $EndTime = Get-Date
                  $ScriptTime = ($EndTime - $StartTime).TotalSeconds
                  $momapi.LogScriptEvent($ScriptName,$EventID,0,"`n Script Completed. `n Script Runtime: ($ScriptTime) seconds.")


                  #add the values to the bag
                  $bag.AddValue('Message', $strMessage)
                  $bag.AddValue('Status', $strStatus)
                  $bag.AddValue('Files',$count)
                  $bag
                </ScriptBody>
                <Parameters>
                  <Parameter>
                    <Name>ID</Name>
                    <Value>$Config/ID$</Value>
                  </Parameter>
                  <Parameter>
                    <Name>CSV</Name>
                    <Value>$Config/CSV$</Value>
                  </Parameter>
                </Parameters>
                <TimeoutSeconds>300</TimeoutSeconds>
              </ProbeAction>
            </MemberModules>
            <Composition>
              <Node ID="PSScript" />
            </Composition>
